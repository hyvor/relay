{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.4",
        "ext-apcu": "*",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-openssl": "*",
        "ext-pdo_pgsql": "*",
        "doctrine/dbal": "^3.10.0",
        "doctrine/doctrine-bundle": "^2.15.1",
        "doctrine/doctrine-migrations-bundle": "^3.4.2",
        "doctrine/orm": "^3.5.0",
        "egulias/email-validator": "^4.0.4",
        "hyvor/internal": "^3.1.2",
        "mlocati/spf-lib": "^3.3",
        "phpdocumentor/reflection-docblock": "^5.6.2",
        "phpmailer/phpmailer": "^6.10",
        "phpstan/phpdoc-parser": "^2.2",
        "runtime/frankenphp-symfony": "^0.2.0",
        "symfony/console": "7.3.*",
        "symfony/dotenv": "7.3.*",
        "symfony/flex": "^2.8.1",
        "symfony/framework-bundle": "7.3.*",
        "symfony/lock": "7.3.*",
        "symfony/messenger": "7.3.*",
        "symfony/mime": "7.3.*",
        "symfony/monolog-bundle": "^3.10",
        "symfony/property-access": "7.3.*",
        "symfony/property-info": "7.3.*",
        "symfony/rate-limiter": "7.3.*",
        "symfony/runtime": "7.3.*",
        "symfony/scheduler": "7.3.*",
        "symfony/serializer": "7.3.*",
        "symfony/uid": "7.3.*",
        "symfony/yaml": "7.3.*"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "bump-after-update": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.3.*",
            "docker": false
        }
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^8.3",
        "phpstan/phpstan": "^2.1.21",
        "phpunit/phpunit": "^12.3.0",
        "symfony/browser-kit": "7.3.*",
        "symfony/css-selector": "7.3.*",
        "symfony/maker-bundle": "^1.64",
        "zenstruck/foundry": "^2.6.1",
        "zenstruck/messenger-test": "^1.11.1"
    }
}
